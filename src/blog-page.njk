---
pagination:
    data: blog
    size: 1
    alias: article
permalink: "{{ article.slug }}"
custom_styles:
    - "article.css"
---


{% extends "base.njk" %}

{% block custom_title %}
{{ article.title | safe }} - Kevin.Gimbel.Dev/Blog
{% endblock %}

{% block head %}
<link rel="canonical" href="{{ article.link }}" />
{% endblock %}

{% block content %}

{% include "nav.njk" %}

<section class="[ f-ultra stack ] [ gr-auto gc-full ] [ c-full-width ]  [ box-shadow__styled ] single-page article-page">
    
    <div class="article__og-box">
        <p><i>"{{ article.title | safe }}"</i> was originally published on <a href="{{ article.link }}" target="_blank" rel="nofollow" title="Read '{{ article.title}}' on kevingimbel.de">kevingimbel.de</a>.</p>
    </div>
    <hr />
    <div class="content">
        <h1>{{ article.title | safe }}</h1>
        {{ article.content | safe }}
    </div>
</section>

{% endblock %}